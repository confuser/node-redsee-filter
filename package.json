{
  "name": "redsee-filter",
  "version": "1.0.0",
  "description": "English profanity, email and url filter backed by redis",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "scripts": {
    "lint": "jshint .",
    "checkStyle": "jscs .",
    "pretest": "npm run-script lint && npm run-script checkStyle",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "posttest": "./node_modules/.bin/istanbul check-coverage && rm -rf coverage",
    "prepublish": "npm test && npm prune"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:confuser/node-redsee-filter"
  },
  "keywords": [
    "filter",
    "profanity",
    "chat",
    "swear",
    "message",
    "redsee",
    "redis",
    "url",
    "urls",
    "email",
    "emails",
    "word",
    "words",
    "phrase",
    "phrases"
  ],
  "author": "James Mortemore <jamesmortemore@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/confuser/node-redsee-filter/issues"
  },
  "dependencies": {
    "async": "^0.9.0",
    "diacritics": "^1.2.1",
    "lodash": "^3.8.0",
    "multimatch": "^2.0.0",
    "natural": "^0.2.1",
    "powerset-stream": "^0.1.0",
    "through2": "^0.6.5",
    "validity-currency": "^1.1.2"
  },
  "devDependencies": {
    "assert-diff": "^1.0.1",
    "coveralls": "^2.11.2",
    "fakeredis": "^0.3.1",
    "istanbul": "0.3.5",
    "redis": "^0.12.1",
    "redis-scanstreams": "^1.0.3",
    "jscs": "1",
    "jshint": "2",
    "jshint-full-path": "0",
    "mocha": "2"
  }
}
