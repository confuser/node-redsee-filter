{
  "name": "redsee-filter",
  "version": "2.0.1",
  "description": "English profanity, email and url filter",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "scripts": {
    "lint": "eslint -f unix .",
    "pretest": "npm run lint",
    "test": "istanbul cover _mocha test",
    "posttest": "(istanbul check-coverage --statements 90 --branches 75 --functions 100 --lines 95) || echo Look at 'coverage/lcov-report/index.html' to find out more",
    "prepublish": "npm test && npm prune"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:confuser/node-redsee-filter"
  },
  "keywords": [
    "filter",
    "profanity",
    "chat",
    "swear",
    "message",
    "redsee",
    "url",
    "urls",
    "email",
    "emails",
    "word",
    "words",
    "phrase",
    "phrases"
  ],
  "author": "James Mortemore <jamesmortemore@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/confuser/node-redsee-filter/issues"
  },
  "dependencies": {
    "async": "^0.9.0",
    "diacritics": "^1.2.1",
    "lodash": "^3.8.0",
    "multimatch": "^2.0.0",
    "natural": "^0.2.1",
    "powerset-stream": "^0.1.0",
    "through2": "^0.6.5",
    "validity-currency": "^1.1.2"
  },
  "devDependencies": {
    "@confuser/eslint-config": "^1.0.0",
    "assert-diff": "^1.0.1",
    "coveralls": "^2.11.2",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "istanbul": "^0.4.1",
    "mocha": "2",
    "stream-spigot": "^3.0.5"
  }
}
